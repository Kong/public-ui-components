      "model": "protocols",
      "order": 0,
      "disabled": false
    },
    {
      "label": "SNI(s)",
      "name": "snis",
      "type": "input",
      "submitWhenNull": true,
      "inputType": "text",
      "valueType": "array",
      "valueArrayType": "string",
      "placeholder": "Enter an SNI",
      "help": "A list of SNIs that match this Route when using stream routing. ",
      "id": "snis",
      "model": "snis",
      "order": 0,
      "disabled": false
    },
    {
