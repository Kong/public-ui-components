{
  "fields": [
    {
      "id": {
        "type": "string",
        "auto": true,
        "uuid": true,
        "description": "A string representing a UUID (universally unique identifier)."
      }
    },
    {
      "created_at": {
        "type": "integer",
        "auto": true,
        "timestamp": true,
        "description": "An integer representing an automatic Unix timestamp in seconds."
      }
    },
    {
      "consumer": {
        "required": true,
        "on_delete": "cascade",
        "reference": "consumers",
        "type": "foreign"
      }
    },
    {
      "key": {
        "required": false,
        "unique": true,
        "auto": true,
        "encrypted": true,
        "type": "string"
      }
    },
    {
      "tags": {
        "type": "set",
        "elements": {
          "type": "string",
          "required": true,
          "description": "A string representing a tag."
        },
        "description": "A set of strings representing tags."
      }
    },
    {
      "ttl": {
        "between": [
          0,
          100000000
        ],
        "description": "Time-to-live (in seconds) value for data",
        "type": "number"
      }
    }
  ]
}
