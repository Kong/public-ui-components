{
  "fields": [
    {
      "id": {
        "uuid": true,
        "type": "string",
        "auto": true
      }
    },
    {
      "created_at": {
        "auto": true,
        "timestamp": true,
        "type": "integer"
      }
    },
    {
      "consumer": {
        "reference": "consumers",
        "type": "foreign",
        "on_delete": "cascade",
        "required": true
      }
    },
    {
      "key": {
        "auto": true,
        "hint": "If left empty, a key will be generated after saving this form.",
        "required": false,
        "type": "string",
        "encrypted": true,
        "unique": true
      }
    },
    {
      "tags": {
        "elements": {
          "type": "string",
          "required": true
        },
        "type": "set"
      }
    },
    {
      "ttl": {
        "between": [
          0,
          100000000
        ],
        "description": "Time-to-live (in seconds) value for data",
        "type": "number"
      }
    }
  ]
}
